<html class="noscroll">
    <%- include("./partials/head") %>
    <body id="body-login">
        <%- include("./partials/nav") %>
        <div id="login-content">
            <div id="login-main">
                <div id="login-text">
                    <h1>Login</h1>
                </div>
                <div id="login-input">
                    <div>
                        <h1>Enter email</h1>
                        <input type="email" name="login" id="login-email" placeholder="Email" class="login">
                    </div>
                    <div>
                        <h1>Enter password</h1>
                        <input type="password" name="password" id="login-password" placeholder="Password" class="login">
                    </div>
               </div>
                <div id="login-buttons">
                    <div>
                        <button class="styled-button" id="left">Create new user</button>
                    </div>
                    <div>
                        <button class="styled-button" id="right">Login</button>
                    </div>
                </div>
            </div>
        </div>
    </body>
</html>